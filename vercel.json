{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/health",
      "dest": "/server-supabase.js"
    },
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
